{
  "name": "halcyon-laravel/base",
  "description": "Base",
  "require": {
    "php": ">=7.1",
    "yajra/laravel-datatables-oracle": "~8.0",
    "spatie/laravel-translatable": "^3.1",
    "spatie/laravel-permission": "^2.23",
    "prettus/l5-repository": "^2.6"
  },
  "require-dev": {
    "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
    "phpunit/phpunit": "^5.7|6.2|^7.0",
    "mockery/mockery": "^1.0",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "HalcyonLaravel\\Base\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HalcyonLaravel\\Base\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HalcyonLaravel\\Base\\Providers\\BaseServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "scripts": {
    "phpunit": [
      "./vendor/bin/phpunit --verbose > phpunit.txt -d memory_limit=2048M"
    ],
    "coverage": [
      "./vendor/bin/phpunit --coverage-html ./coverage"
    ],
    "coverage-txt": [
      "./vendor/bin/phpunit --coverage-text --coverage-clover=coverage.clover"
    ],
    "test": "./vendor/bin/phpunit --verbose"
  }
}
